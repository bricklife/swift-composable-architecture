reciseIdentifier":"s:SH"},{"text":", ","kind":"text"},{"text":"ID","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"Sendable","preciseIdentifier":"s:s8SendableP","kind":"typeIdentifier"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"Element"},{"text":" : ","kind":"text"},{"kind":"typeIdentifier","text":"Reducer","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer","preciseIdentifier":"s:22ComposableArchitecture7ReducerP"},{"text":"\n","kind":"text"}]}]},{"parameters":[{"content":[{"inlineContent":[{"type":"text","text":"A writable key path from parent state to an "},{"type":"codeVoice","code":"IdentifiedArray"},{"type":"text","text":" of child"},{"text":" ","type":"text"},{"type":"text","text":"state."}],"type":"paragraph"}],"name":"toElementsState"},{"name":"toElementAction","content":[{"inlineContent":[{"text":"A case path from parent action to an ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/IdentifiedAction"},{"type":"text","text":" of child"},{"text":" ","type":"text"},{"text":"actions.","type":"text"}],"type":"paragraph"}]},{"name":"element","content":[{"type":"paragraph","inlineContent":[{"text":"A reducer that will be invoked with child actions against elements of child","type":"text"},{"type":"text","text":" "},{"text":"state.","type":"text"}]}]}],"kind":"parameters"},{"kind":"content","content":[{"type":"heading","level":2,"anchor":"return-value","text":"Return Value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"A reducer that combines the child reducer with the parent reducer."}]}]},{"content":[{"level":2,"text":"Discussion","type":"heading","anchor":"discussion"},{"inlineContent":[{"type":"text","text":"For example, if a parent feature holds onto an array of child states, then it can perform"},{"type":"text","text":" "},{"text":"its core logic ","type":"text"},{"type":"emphasis","inlineContent":[{"text":"and","type":"text"}]},{"type":"text","text":" the child’s logic by using the "},{"type":"codeVoice","code":"forEach"},{"text":" operator:","type":"text"}],"type":"paragraph"},{"syntax":"swift","code":["@Reducer","struct Parent {","  struct State {","    var rows: IdentifiedArrayOf<Row.State>","    \/\/ ...","  }","  enum Action {","    case rows(IdentifiedActionOf<Row>)","    \/\/ ...","  }","","  var body: some Reducer<State, Action> {","    Reduce { state, action in","      \/\/ Core logic for parent feature","    }","    .forEach(\\.rows, action: \\.rows) {","      Row()","    }","  }","}"],"type":"codeListing"},{"style":"tip","content":[{"inlineContent":[{"text":"We are using ","type":"text"},{"type":"codeVoice","code":"IdentifiedArray"},{"text":" from our","type":"text"},{"text":" ","type":"text"},{"identifier":"http:\/\/github.com\/pointfreeco\/swift-identified-collections","isActive":true,"type":"reference"},{"text":" library because it provides a safe","type":"text"},{"type":"text","text":" "},{"text":"and ergonomic API for accessing elements from a stable ID rather than positional indices.","type":"text"}],"type":"paragraph"}],"name":"Tip","type":"aside"},{"type":"paragraph","inlineContent":[{"text":"The ","type":"text"},{"code":"forEach","type":"codeVoice"},{"type":"text","text":" forces a specific order of operations for the child and parent features. It"},{"type":"text","text":" "},{"text":"runs the child first, and then the parent. If the order was reversed, then it would be","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"possible for the parent feature to remove the child state from the array, in which case the"},{"type":"text","text":" "},{"type":"text","text":"child feature would not be able to react to that action. That can cause subtle bugs."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"It is still possible for a parent feature higher up in the application to remove the child"},{"type":"text","text":" "},{"type":"text","text":"state from the array before the child has a chance to react to the action. In such cases a"},{"type":"text","text":" "},{"text":"runtime warning is shown in Xcode to let you know that there’s a potential problem.","type":"text"}]}],"kind":"content"}],"schemaVersion":{"minor":3,"major":0,"patch":0},"hierarchy":{"paths":[["doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Scope","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Scope\/Reducer-Implementations"]]},"sections":[],"abstract":[{"text":"Embeds a child reducer in a parent domain that works on elements of a collection in parent","type":"text"},{"text":" ","type":"text"},{"text":"state.","type":"text"}],"references":{"http://github.com/pointfreeco/swift-identified-collections":{"identifier":"http:\/\/github.com\/pointfreeco\/swift-identified-collections","title":"Identified Collections","titleInlineContent":[{"type":"text","text":"Identified Collections"}],"url":"http:\/\/github.com\/pointfreeco\/swift-identified-collections","type":"link"},"doc://ComposableArchitecture/documentation/ComposableArchitecture/Effect":{"url":"\/documentation\/composablearchitecture\/effect","title":"Effect","navigatorTitle":[{"kind":"identifier","text":"Effect"}],"kind":"symbol","role":"symbol","abstract":[],"type":"topic","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Effect","fragments":[{"kind":"keyword","text":"struct"},{"text":" ","kind":"text"},{"text":"Effect","kind":"identifier"}]},"doc://ComposableArchitecture/documentation/ComposableArchitecture":{"abstract":[{"text":"The Composable Architecture (TCA, for short) is a library for building applications in a consistent","type":"text"},{"type":"text","text":" "},{"text":"and understandable way, with compos